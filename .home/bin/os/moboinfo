#!/usr/bin/env bash
set -e

whereami=`whereami`
if [[ "$whereami" == 'darwin' ]]; then echo "`system_profiler SPHardwareDataType | grep "Model Identifier" | gawk '{print $3}'`" # grep "Model Identifier\|Model Number"
elif [[ "$whereami" == 'windows' ]]; then wmic baseboard get product,Manufacturer
else sudo dmidecode -s system-product-name; fi
