#!/usr/bin/env bash
set -f
charname="`to lowercase $1`"
if [[ "$charname" =~ 'x'|'*' ]]; then echo '×'
elif [[ "$charname" == '.' ]]; then echo '•'
elif [[ "$charname" == '|' ]]; then echo '│'
elif [[ "$charname" == '/' ]]; then echo '÷'
elif [[ "$charname" == '-' ]]; then echo '−'
elif [[ "$charname" == 'deg'* ]]; then echo '°'
elif [[ "$charname" == '?' ]]; then echo '¿'
elif [[ "$charname" == 'c' ]]; then echo '©'
elif [[ "$charname" == 'r' ]]; then echo '®'
elif [[ "$charname" == 'tm' ]]; then echo '™'
elif [[ "$charname" == '^2' ]]; then echo '²'
elif [[ "$charname" == '^3' ]]; then echo '³'
elif [[ "$charname" == '<' ]]; then echo '‹'
elif [[ "$charname" == '>' ]]; then echo '›'
elif [[ "$charname" == '<<' ]]; then echo '«'
elif [[ "$charname" == '>>' ]]; then echo '»'
elif [[ "$charname" == '...' ]]; then echo '…'
elif [[ "$charname" == 'alpha' ]]; then echo 'α'
elif [[ "$charname" == 'beta' ]]; then echo 'β'
elif [[ "$charname" == 'chi' ]]; then echo 'χ'
elif [[ "$charname" == 'delta' ]]; then echo 'δ'
elif [[ "$charname" == 'epsilon' ]]; then echo 'ε'
elif [[ "$charname" == 'eta' ]]; then echo 'η'
elif [[ "$charname" == 'gamma' ]]; then echo 'γ'
elif [[ "$charname" == 'iota' ]]; then echo 'ι'
elif [[ "$charname" == 'kappa' ]]; then echo 'κ'
elif [[ "$charname" == 'lambda' ]]; then echo 'λ'
elif [[ "$charname" == 'mu' ]]; then echo 'μ'
elif [[ "$charname" == 'nu' ]]; then echo 'ν'
elif [[ "$charname" == 'omega' ]]; then echo 'ω'
elif [[ "$charname" == 'omicron' ]]; then echo 'ο'
elif [[ "$charname" == 'phi' ]]; then echo 'φ'
elif [[ "$charname" == 'pi' ]]; then echo 'π'
elif [[ "$charname" == 'psi' ]]; then echo 'ψ'
elif [[ "$charname" == 'rho' ]]; then echo 'ρ'
elif [[ "$charname" == 'sigma' ]]; then echo 'σ'
elif [[ "$charname" == 'tau' ]]; then echo 'τ'
elif [[ "$charname" == 'theta' ]]; then echo 'θ'
elif [[ "$charname" == 'upsilon' ]]; then echo 'υ'
elif [[ "$charname" == 'xi' ]]; then echo 'ξ'
elif [[ "$charname" == 'zeta' ]]; then echo 'ζ'
elif [[ "$charname" == 'check' ]]; then echo '✓'
fi
